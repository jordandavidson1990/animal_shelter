<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Age</th>
    <th>Type</th>
    <th>Breed</th>
    <th>Admission Date</th>
    <th>Status</th>
  </tr>
  <% for pet in @pets %>
  <tr>
    <td><a href="/pets/<%= pet.id %>">
    <%= pet.name %></a></td>
    <td>|</td>
    <td><%= pet.age %></td>
    <td><%= pet.type %></td>
    <td><%= pet.breed %></td>
    <td><%= pet.admission_date %></td>
    <td><%= pet.status%></td>
  </tr>
  <% end %>
</table>
