                <td><a href="/pets/<%= pet.id %>"><%= pet.breed %></td>
                  <td><a href="/pets/<%= pet.id %>"><%= pet.admission_date %></td>
                    <td><a href="/pets/<%= pet.id %>"><%= pet.status%></td>
                    </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
