<h1>Proud Owners</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>

  </tr>
  <% for owner in @owners %>
  <tr>
    <td><a href="/owners/<%= owner.id %>"><%= owner.name %></a></td>

  </tr>
  <% end %>
</table>

<form method="get" action="/owners/new">
 <input type="submit" value="Add Owner"/>
</form>
